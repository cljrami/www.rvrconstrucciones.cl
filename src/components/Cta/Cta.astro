---
export interface Props {
  h2: string;
  sectionClasses?: string;
  h2Classes?: string;
  btnClasses?: string;
  txtBtn?: string;
}

const {
  h2,
  sectionClasses = "",
  h2Classes = "",
  btnClasses = "",
  txtBtn = "",
} = Astro.props;
import TarjetaClientes from "../Clientes/TarjetaClientes.astro";
---

<section class="w-full flex" class={`${sectionClasses}`} id="Descarga">
  <div class="max-w-7xl mx-auto px-2 py-10">
    <div class="text-center space-y-5">
      <h2 class={`${h2Classes}`} class="">
        {h2}
      </h2>
      <div
        class="inline-flex items-end justify-center w-full text-center mx-auto"
      >
        <img
          src="data:image/jpeg;base64,"
          class="absolute transform translate-x-24 ml-6 rounded-full w-12 h-12 md:w-16 md:h-16 border-4 border-white"
        />
        <img
          src="data:image/png;base64,"
          class="absolute transform -translate-x-24 -ml-6 rounded-full w-12 h-12 md:w-16 md:h-16 border-4 border-white"
        />
        <img
          src="data:image/png;base64,"
          class="absolute transform -translate-x-16 rounded-full w-16 h-16 md:w-20 md:h-20 border-4 border-white"
        />
        <img
          src="data:image/jpeg;base64,"
          class="absolute transform translate-x-16 rounded-full w-16 h-16 md:w-20 md:h-20 border-4 border-white"
        />
        <img
          src="data:image/png;base64,"
          class="rounded-full w-20 h-20 md:w-24 md:h-24 border-4 border-white relative"
        />
      </div>

      <div
        class="max-w-5xl grid grid-cols-2 gap-4 pt-10 sm:grid-cols-2 md:grid-cols-2 lg:grid-cols-4 justify-center"
      >
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/arauco.webp"
            alt="Arauco"
            loading="lazy"
            href="https://www.arauco.cl"
            title="Dorestal Arauco"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/losheroes.webp"
            alt="Caja De Componsación Los Heros"
            loading="lazy"
            href="https://www.losheroes.cl"
            title="Caja De Componsación Los Heros"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/orizon.webp"
            alt="Pesquera Orizon"
            loading="lazy"
            href="https://www.orizon.cl"
            title="Pesquera Orizon"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/araucocoronel.webp"
            alt="Mall Arauco Coronel"
            loading="lazy"
            href="https://www.araucocoronel.cl"
            title="Mall Arauco Coronel"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/coemco.webp"
            alt="Corporación Educacional Masónica De Concepción"
            loading="lazy"
            href="https://www.coemco.cl"
            title="Corporación Educacional Masónica De Concepción"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/duoc.webp"
            alt="Instituto Profesional DUOC UC"
            loading="lazy"
            href="https://www.duoc.cl"
            title="Instituto Profesional DUOC UC"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/hites.webp"
            alt="Multitienda Hites"
            loading="lazy"
            href="https://www.hites.cl"
            title="Multitienda Hites"
            target="_blank"
          />
        </div>
        <div class="w-42">
          <TarjetaClientes
            additionalClasses="object-contain w-full mx-auto rounded-md "
            src="/Imagenes/clientes/sercotec.webp"
            alt="Sercotec"
            loading="lazy"
            href="https://www.sercotec.cl"
            title="Sercotec"
            target="_blank"
          />
        </div>
      </div>
    </div>

    <div class="mt-8 flex justify-center">
      <div class="inline-flex rounded-md shadow">
        <a href="/Pdfs/Brochure2024.pdf" class={`${btnClasses}`}>
          {txtBtn}
        </a>
      </div>
    </div>
  </div>
</section>
