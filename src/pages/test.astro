---
import Layout from "../layouts/Layout.astro";
import { AstroSeo } from "@astrolib/seo";
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />

    <AstroSeo
      title="Verdaderos Expertos En Construccion y Proyectos Relacionados"
      description=""
      canonical="https://www.rvrconstrucciones.cl/"
      openGraph={{
        url: "https://www.rvrconstrucciones.cl/",
        type: "website",
        title: "Verdaderos Expertos En Construccion y Proyectos Relacionados",
        description:
          "Somos una Empresa dedicada al Servicio de Ingeniería, Construcción y Mantenimiento de Infraestructura",
        images: [
          {
            url: "https://www.rvrconstrucciones.cl/OgImage.webp",
            width: 1240,
            height: 630,
            alt: "",
            type: "image/webp",
          },
        ],
        site_name: "",
      }}
      twitter={{
        handle: "@handle",
        site: "@site",
        cardType: "summary_large_image",
      }}
    />
    <script type="application/ld+json" is:inline>
      {
        "@context": "https://schema.org",
        "@type": "BreadcrumbList",
        "itemListElement": [
          {
            "@type": "ListItem",
            "position": 1,
            "name": "",
            "item": "www.rvrconstrucciones.cl"
          }
        ]
      }
    </script>
  </head>

  <Layout title="" />


</html>
